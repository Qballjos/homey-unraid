{
  "name": "com.unraid",
  "version": "0.4.2",
  "description": "Lightweight Homey app to monitor and control an Unraid server via the official GraphQL API.",
  "main": "app.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "validate": "homey app validate --level publish",
    "run": "homey app run",
    "deploy": "homey app install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node test-local.js",
    "test:all": "npm run lint && npm run test && npm run validate"
  },
  "keywords": [
    "homey",
    "unraid",
    "smart-home",
    "home-automation",
    "docker",
    "graphql",
    "nas",
    "server-monitoring"
  ],
  "author": {
    "name": "Jos Visser",
    "email": "qballjos@gmail.com",
    "url": "https://josvisserict.nl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Qballjos/homey-unraid.git"
  },
  "bugs": {
    "url": "https://github.com/Qballjos/homey-unraid/issues"
  },
  "homepage": "https://github.com/Qballjos/homey-unraid#readme",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^8.57.1"
  }
}
